                                           }\r\n                                                    \r\n                                                        \/\/ Inicia os countdowns quando a p\u00e1gina carrega\r\n                                                        window.onload = function() {\r\n                                                          countdownTimer(84, 'countdown');\r\n                                                          countdownTimer(66, 'countdownFamilia');\r\n                                                        };\r\n                                                      <\/script>\r\n\r\n<div class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"card-wrapper\">\r\n        <div class=\"card\">\r\n          <img decoding=\"async\" src=\"https:\/\/down-br.img.susercontent.com\/file\/br-11134207-7r98o-lml3ynk8y1h224\" alt=\"img\">\r\n          <div class=\"text-content\">\r\n            <h3 class=\"product-title\" style=\"text-align: center;\">\r\n                Kit Material Escolar Masculino\r\n               <\/h3>\r\n               <p style=\"text-align: center;\"><strong>45 itens - Confira na descri\u00e7\u00e3o<\/strong><\/p>\r\n               <p class=\"product-description\">\r\n                <i>01 Super Mochila PREMIUM Cabo de A\u00e7o Compartimento para Notebook.<\/i><br>\r\n                <i>01 Agenda, 200 folhas, Capa DURA.<\/i><br>\r\n                <i>04 Cadernos, 200 folhas, Capa DURA.<\/i><br>\r\n                <i>01 Estojo\/Penal, Com Z\u00edper refor\u00e7ado.<\/i><br>\r\n                <i>12 L\u00e1pis de Cor Faber Castell.<\/i><br>\r\n                <i>06 Caneta Fineliner Colorida Faber Castell, ponta fina.<\/i><br>\r\n                <i>06 L\u00e1pis de Cor (Tom pastel).<\/i><br>\r\n                <i>04 Bloco de notas.<\/i><br>\r\n                <i>01 Lapiseira Faber Castell, Grafite 05mm.<\/i><br>\r\n                <i>01 Apontador Com Lixeira.<\/i><br>\r\n                <i>04 R\u00e9